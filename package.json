{
  "name": "@mb-labs/desafio-nicolas-ribeiro",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "ci": "pnpm lint:apply && pnpm test",
    "lint": "biome check apps packages",
    "lint:apply": "biome check apps packages --apply",
    "test": "echo \"no test environment set up\"\n",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@mb-labs/typescript-config": "workspace:*",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^8.0.0",
    "prettier": "^3.1.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
